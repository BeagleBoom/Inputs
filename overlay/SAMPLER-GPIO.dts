/dts-v1/;
/plugin/;

/{
       compatible = "ti,beaglebone", "ti,beaglebone-black";
       part-number = "SAMPLER-GPIO";
       version = "00A0";

       fragment@0 {
             target = <&am33xx_pinmux>;
            
             __overlay__ {
                  pinctrl_test: DM_GPIO_Test_Pins {
			pinctrl-single,pins = <
				0x078 0x37  /* P9_12 60 INPUT  MODE7 PULLUP*/
				0x184 0x37  /* P9_24 15 INPUT  MODE7 PULLUP*/
				0x034 0x37  /* P8_11 45 INPUT  MODE7 PULLUP*/
				0x030 0x37  /* P8_12 44 INPUT  MODE7 PULLUP*/
				0x024 0x37  /* P8_13 23 INPUT  MODE7 PULLUP*/
			>;
		  };
             };
       };

       fragment@1 {
		target = <&ocp>;
		__overlay__ {
			gpio_helper: helper {
				compatible = "gpio-of-helper";
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_test>;
				status = "okay";
			};
		};
	};
};
